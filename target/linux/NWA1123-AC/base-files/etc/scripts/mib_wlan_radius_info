echo "$RAD_NAME_2,$PRI_SERVER_2,$PRI_SERVER_IP_2,$PRI_SERVER_PORT_2,$PRI_SERVER_SECRET_2,$ACCT_PRI_SERVER_2,$ACCT_PRI_SERVER_IP_2,$ACCT_PRI_SERVER_PORT_2,$ACCT_PRI_SERVER_SECRET_2,$SEC_SERVER_2,$SEC_SERVER_IP_2,$SEC_SERVER_PORT_2,$SEC_SERVER_SECRET_2,$ACCT_SEC_SERVER_2,$ACCT_SEC_SERVER_IP_2,$ACCT_SEC_SERVER_PORT_2,$ACCT_SEC_SERVER_SECRET_2"
