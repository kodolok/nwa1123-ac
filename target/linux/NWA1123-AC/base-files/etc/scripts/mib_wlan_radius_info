then
    ACCT_PRI_SERVER_2=1
else
    ACCT_PRI_SERVER_2=2
fi

if [ "$ACCT_PRI_SERVER_STR_3" = "Active" ]
then
    ACCT_PRI_SERVER_3=1
else
    ACCT_PRI_SERVER_3=2
fi

if [ "$ACCT_PRI_SERVER_STR_4" = "Active" ]
then
    ACCT_PRI_SERVER_4=1
else
    ACCT_PRI_SERVER_4=2
fi

if [ "$ACCT_PRI_SERVER_IP_1" = "" ]
then
    ACCT_PRI_SERVER_IP_1="unknown"
fi

if [ "$ACCT_PRI_SERVER_IP_2" = "" ]
then
    ACCT_PRI_SERVER_IP_2="unknown"
fi

if [ "$ACCT_PRI_SERVER_IP_3" = "" ]
then
    ACCT_PRI_SERVER_IP_3="unknown"
fi

