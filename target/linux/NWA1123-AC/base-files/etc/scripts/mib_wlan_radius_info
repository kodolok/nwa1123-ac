ACCT_PRI_SERVER_IP_4=`cfg -v RAD_ACCT_PRI_SERVER_IP_4`
ACCT_PRI_SERVER_PORT_1=`cfg -v RAD_ACCT_PRI_SERVER_PORT_1`
ACCT_PRI_SERVER_PORT_2=`cfg -v RAD_ACCT_PRI_SERVER_PORT_2`
