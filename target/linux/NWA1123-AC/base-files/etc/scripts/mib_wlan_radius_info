then
    SEC_SERVER_2=1
else
    SEC_SERVER_2=2
fi

if [ "$SEC_SERVER_STR_3" = "Active" ]
then
    SEC_SERVER_3=1
else
    SEC_SERVER_3=2
fi

if [ "$SEC_SERVER_STR_4" = "Active" ]
then
    SEC_SERVER_4=1
else
    SEC_SERVER_4=2
