then
    ACCT_SEC_SERVER_1=1
else
    ACCT_SEC_SERVER_1=2
fi

if [ "$ACCT_SEC_SERVER_STR_2" = "Active" ]
then
    ACCT_SEC_SERVER_2=1
else
    ACCT_SEC_SERVER_2=2
fi

if [ "$ACCT_SEC_SERVER_STR_3" = "Active" ]
then
    ACCT_SEC_SERVER_3=1
else
    ACCT_SEC_SERVER_3=2
fi

if [ "$ACCT_SEC_SERVER_STR_4" = "Active" ]
then
    ACCT_SEC_SERVER_4=1
else
    ACCT_SEC_SERVER_4=2
fi

####
if [ "$ACCT_SEC_SERVER_IP_1" = "" ]
then
    ACCT_SEC_SERVER_IP_1="unknown"
fi

if [ "$ACCT_SEC_SERVER_IP_2" = "" ]
then
    ACCT_SEC_SERVER_IP_2="unknown"
fi

if [ "$ACCT_SEC_SERVER_IP_3" = "" ]
then
    ACCT_SEC_SERVER_IP_3="unknown"
fi

if [ "$ACCT_SEC_SERVER_IP_4" = "" ]
then
    ACCT_SEC_SERVER_IP_4="unknown"
fi

if [ "$ACCT_SEC_SERVER_PORT_1" = "" ]
then
    ACCT_SEC_SERVER_PORT_1="unknown"
fi

if [ "$ACCT_SEC_SERVER_PORT_2" = "" ]
then
    ACCT_SEC_SERVER_PORT_2="unknown"
fi

if [ "$ACCT_SEC_SERVER_PORT_3" = "" ]
then
    ACCT_SEC_SERVER_PORT_3="unknown"
fi

if [ "$ACCT_SEC_SERVER_PORT_4" = "" ]
then
    ACCT_SEC_SERVER_PORT_4="unknown"
fi

if [ "$ACCT_SEC_SERVER_SECRET_1" = "" ]
then
    ACCT_SEC_SERVER_SECRET_1="unknown"
fi

if [ "$ACCT_SEC_SERVER_SECRET_2" = "" ]
then
    ACCT_SEC_SERVER_SECRET_2="unknown"
fi

if [ "$ACCT_SEC_SERVER_SECRET_3" = "" ]
then
    ACCT_SEC_SERVER_SECRET_3="unknown"
fi

if [ "$ACCT_SEC_SERVER_SECRET_4" = "" ]
then
    ACCT_SEC_SERVER_SECRET_4="unknown"
fi


