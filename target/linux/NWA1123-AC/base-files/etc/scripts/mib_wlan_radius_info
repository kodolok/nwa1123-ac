echo "$RAD_NAME_4,$PRI_SERVER_4,$PRI_SERVER_IP_4,$PRI_SERVER_PORT_4,$PRI_SERVER_SECRET_4,$ACCT_PRI_SERVER_4,$ACCT_PRI_SERVER_IP_4,$ACCT_PRI_SERVER_PORT_4,$ACCT_PRI_SERVER_SECRET_4,$SEC_SERVER_4,$SEC_SERVER_IP_4,$SEC_SERVER_PORT_4,$SEC_SERVER_SECRET_4,$ACCT_SEC_SERVER_4,$ACCT_SEC_SERVER_IP_4,$ACCT_SEC_SERVER_PORT_4,$ACCT_SEC_SERVER_SECRET_4"

