SEC_SERVER_STR_2=`cfg -v RAD_AUTH_SEC_SERVER_2`
SEC_SERVER_STR_3=`cfg -v RAD_AUTH_SEC_SERVER_3`
SEC_SERVER_STR_4=`cfg -v RAD_AUTH_SEC_SERVER_4`
SEC_SERVER_IP_1=`cfg -v RAD_AUTH_SEC_SERVER_IP_1`
SEC_SERVER_IP_2=`cfg -v RAD_AUTH_SEC_SERVER_IP_2`
SEC_SERVER_IP_3=`cfg -v RAD_AUTH_SEC_SERVER_IP_3`
SEC_SERVER_IP_4=`cfg -v RAD_AUTH_SEC_SERVER_IP_4`
SEC_SERVER_PORT_1=`cfg -v RAD_AUTH_SEC_SERVER_PORT_1`
SEC_SERVER_PORT_2=`cfg -v RAD_AUTH_SEC_SERVER_PORT_2`
SEC_SERVER_PORT_3=`cfg -v RAD_AUTH_SEC_SERVER_PORT_3`
SEC_SERVER_PORT_4=`cfg -v RAD_AUTH_SEC_SERVER_PORT_4`
