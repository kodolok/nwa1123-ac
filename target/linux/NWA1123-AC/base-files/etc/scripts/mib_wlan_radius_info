ACCT_PRI_SERVER_SECRET_3=`cfg -v RAD_ACCT_PRI_SERVER_SECRET_3`
ACCT_PRI_SERVER_SECRET_4=`cfg -v RAD_ACCT_PRI_SERVER_SECRET_4`

