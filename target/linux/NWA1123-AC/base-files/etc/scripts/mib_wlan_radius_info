then
    ACCT_PRI_SERVER_PORT_1="unknown"
fi

if [ "$ACCT_PRI_SERVER_PORT_2" = "" ]
then
    ACCT_PRI_SERVER_PORT_2="unknown"
fi

if [ "$ACCT_PRI_SERVER_PORT_3" = "" ]
then
    ACCT_PRI_SERVER_PORT_3="unknown"
fi

if [ "$ACCT_PRI_SERVER_PORT_4" = "" ]
then
    ACCT_PRI_SERVER_PORT_4="unknown"
fi

if [ "$ACCT_PRI_SERVER_SECRET_1" = "" ]
then
    ACCT_PRI_SERVER_SECRET_1="unknown"
fi

if [ "$ACCT_PRI_SERVER_SECRET_2" = "" ]
then
    ACCT_PRI_SERVER_SECRET_2="unknown"
fi

if [ "$ACCT_PRI_SERVER_SECRET_3" = "" ]
then
    ACCT_PRI_SERVER_SECRET_3="unknown"
