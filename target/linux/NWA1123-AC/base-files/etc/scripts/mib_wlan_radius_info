ACCT_SEC_SERVER_STR_3=`cfg -v RAD_ACCT_SEC_SERVER_3`
ACCT_SEC_SERVER_STR_4=`cfg -v RAD_ACCT_SEC_SERVER_4`
ACCT_SEC_SERVER_IP_1=`cfg -v RAD_ACCT_SEC_SERVER_IP_1`
ACCT_SEC_SERVER_IP_2=`cfg -v RAD_ACCT_SEC_SERVER_IP_2`
ACCT_SEC_SERVER_IP_3=`cfg -v RAD_ACCT_SEC_SERVER_IP_3`
ACCT_SEC_SERVER_IP_4=`cfg -v RAD_ACCT_SEC_SERVER_IP_4`
ACCT_SEC_SERVER_PORT_1=`cfg -v RAD_ACCT_SEC_SERVER_PORT_1`
ACCT_SEC_SERVER_PORT_2=`cfg -v RAD_ACCT_SEC_SERVER_PORT_2`
ACCT_SEC_SERVER_PORT_3=`cfg -v RAD_ACCT_SEC_SERVER_PORT_3`
ACCT_SEC_SERVER_PORT_4=`cfg -v RAD_ACCT_SEC_SERVER_PORT_4`
ACCT_SEC_SERVER_SECRET_1=`cfg -v RAD_ACCT_SEC_SERVER_SECRET_1`
ACCT_SEC_SERVER_SECRET_2=`cfg -v RAD_ACCT_SEC_SERVER_SECRET_2`
ACCT_SEC_SERVER_SECRET_3=`cfg -v RAD_ACCT_SEC_SERVER_SECRET_3`
ACCT_SEC_SERVER_SECRET_4=`cfg -v RAD_ACCT_SEC_SERVER_SECRET_4`

