#!/bin/sh
####################################################################
## MIB script
## 
## This script is used to set/get the wlanRadiusInfo. 
##
## mib_wlan_radius_info
##
####################################################################
ERR_SUCCESS=0
ERR_ILLEGAL_ARG=1
ERR_NO_FILE=2

##
## RADIUS Server Profile Name Settings
##
RAD_NAME_1=`cfg -v RAD_PRO_NAME_1`
RAD_NAME_2=`cfg -v RAD_PRO_NAME_2`
RAD_NAME_3=`cfg -v RAD_PRO_NAME_3`
RAD_NAME_4=`cfg -v RAD_PRO_NAME_4`

##
## RADIUS Primary Server Info Settings
##
PRI_SERVER_STR_1=`cfg -v RAD_AUTH_PRI_SERVER_1`
PRI_SERVER_STR_2=`cfg -v RAD_AUTH_PRI_SERVER_2`
PRI_SERVER_STR_3=`cfg -v RAD_AUTH_PRI_SERVER_3`
PRI_SERVER_STR_4=`cfg -v RAD_AUTH_PRI_SERVER_4`
PRI_SERVER_IP_1=`cfg -v RAD_AUTH_PRI_SERVER_IP_1`
PRI_SERVER_IP_2=`cfg -v RAD_AUTH_PRI_SERVER_IP_2`
PRI_SERVER_IP_3=`cfg -v RAD_AUTH_PRI_SERVER_IP_3`
PRI_SERVER_IP_4=`cfg -v RAD_AUTH_PRI_SERVER_IP_4`
PRI_SERVER_PORT_1=`cfg -v RAD_AUTH_PRI_SERVER_PORT_1`
PRI_SERVER_PORT_2=`cfg -v RAD_AUTH_PRI_SERVER_PORT_2`
PRI_SERVER_PORT_3=`cfg -v RAD_AUTH_PRI_SERVER_PORT_3`
PRI_SERVER_PORT_4=`cfg -v RAD_AUTH_PRI_SERVER_PORT_4`
PRI_SERVER_SECRET_1=`cfg -v RAD_AUTH_PRI_SERVER_SECRET_1`
PRI_SERVER_SECRET_2=`cfg -v RAD_AUTH_PRI_SERVER_SECRET_2`
PRI_SERVER_SECRET_3=`cfg -v RAD_AUTH_PRI_SERVER_SECRET_3`
PRI_SERVER_SECRET_4=`cfg -v RAD_AUTH_PRI_SERVER_SECRET_4`

if [ "$PRI_SERVER_STR_1" = "Active" ]
then
    PRI_SERVER_1=1
else
    PRI_SERVER_1=2
fi

if [ "$PRI_SERVER_STR_2" = "Active" ]
then
    PRI_SERVER_2=1
else
    PRI_SERVER_2=2
fi

if [ "$PRI_SERVER_STR_3" = "Active" ]
then
    PRI_SERVER_3=1
else
    PRI_SERVER_3=2
fi

if [ "$PRI_SERVER_STR_4" = "Active" ]
then
    PRI_SERVER_4=1
else
    PRI_SERVER_4=2
