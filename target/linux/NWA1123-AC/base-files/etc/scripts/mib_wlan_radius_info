    SEC_SERVER_SECRET_2="unknown"
fi

if [ "$SEC_SERVER_SECRET_3" = "" ]
then
    SEC_SERVER_SECRET_3="unknown"
fi

if [ "$SEC_SERVER_SECRET_4" = "" ]
